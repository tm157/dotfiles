{
  "editor.renderLineHighlight": "none",
  "editor.minimap.enabled": false,
  "editor.tabCompletion": "on",
  "editor.parameterHints.enabled": false,
  "python.linting.pylintEnabled": false,
  "path-intellisense.absolutePathToWorkspace": false,
  "workbench.editor.enablePreview": false,
  // open already opened file in any group
  "workbench.editor.revealIfOpen": true,
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "editor.snippetSuggestions": "none",
  "editor.occurrencesHighlight": "off",
  "latex-workshop.latex.autoBuild.run": "never",
  "latex-workshop.view.pdf.viewer": "tab",
  // "terminal.integrated.fontFamily": "Consolas, Inconsolata, monospace",
  "[python]": {
    // "editor.rulers": [80, 100],
    "editor.rulers": [
      80
    ],
    "editor.autoClosingBrackets": "never",
    "editor.formatOnPaste": false,
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "editor.guides.indentation": true,
    "editor.formatOnType": true,
  },
  "[latex]": {
    "editor.rulers": [
      100
    ],
    "editor.fontFamily": "Monaco, 'Courier New', monospace"
    // "editor.fontFamily": "DejaVu Sans Mono, 'Courier New', monospace"
  },
  "editor.formatOnPaste": true,
  "vim.leader": "<Space>",
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.searchHighlightColor": "#3B4158",
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        ";",
        ";"
      ],
      "after": [
        "<ESC>"
      ]
    },
  ],
  "vim.visualModeKeyBindings": [
    {
      // "before": ["<leader>","c","<space>"],
      "before": [
        "g",
        "c"
      ],
      "commands": [
        "editor.action.commentLine"
      ],
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<leader>",
        "c",
        "<space>"
      ],
      "commands": [
        "editor.action.commentLine"
      ],
    },
    {
      "before": [
        "<Leader>",
        "j"
      ],
      "after": [
        "C-d"
      ]
    },
    {
      "before": [
        "<Leader>",
        ";",
        ";"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<Leader>",
        "k"
      ],
      "after": [
        "C-u"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "after": [
        "<C-w>",
        "h"
      ],
      "when": "vim.active && vim.use<C-w>",
    },
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<C-w>",
        "j"
      ],
      "when": "vim.active && vim.use<C-w>",
    },
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<C-w>",
        "k"
      ],
      "when": "vim.active && vim.use<C-w>",
    },
    {
      "before": [
        "<C-l>"
      ],
      "after": [
        "<C-w>",
        "l"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "t"
      ],
      "commands": [
        ":tabnew"
      ]
    },
    {
      "before": [
        "Z",
        "Z"
      ],
      "commands": [
        ":wq"
      ]
    },
    {
      "before": [
        "<Leader>",
        "?"
      ],
      "commands": [
        "editor.action.goToDeclaration"
      ],
      "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
      "before": [
        "<Leader>",
        "/"
      ],
      "commands": [
        "editor.action.peekDefinition"
      ],
      "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
    {
      "before": [
        "<Leader>",
        "l"
      ],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ],
      // "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
    },
  ],
  "workbench.colorCustomizations": {
    "statusBar.background": "#262626",
    "statusBar.noFolderBackground": "#212121",
    "statusBar.debuggingBackground": "#263238",
    "tab.activeBorderTop": "#007ACC"
  },
  "editor.largeFileOptimizations": false,
  "terminal.integrated.inheritEnv": false,
  "workbench.startupEditor": "welcomePage",
